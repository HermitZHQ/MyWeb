{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "name": "Launch CTSpider",
            "url": "http://127.0.0.1:63342/ctspider/ctspider.html",
            "webRoot": "${workspaceRoot}/CTSpider"
        },
        {
            "type": "chrome",
            "request": "launch",
            "sourceMaps": true,
            "name": "Launch crawler.html",
            "url": "http://127.0.0.1:63342/crawler/crawler.html",
            "webRoot": "${workspaceRoot}/Crawler"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch ts Program",
            "program": "${workspaceRoot}/greeter.js"
        },        
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch Chrome with main.html",
            "url": "http://localhost:63342/main.html",
            "webRoot": "${workspaceRoot}"
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 9222,
            "webRoot": "${workspaceRoot}"
        }
    ]
}